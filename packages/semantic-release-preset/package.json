{
    "name": "@anolilab/semantic-release-preset",
    "version": "2.2.1",
    "description": "Anolilab Coding Standard for semantic-release.",
    "keywords": [
        "anolilab",
        "semantic-release",
        "semantic-release-config",
        "changelog",
        "publish",
        "release",
        "npm",
        "github",
        "git"
    ],
    "homepage": "https://anolilab.com/nodejs/packages/semantic-release-preset",
    "repository": {
        "type": "git",
        "url": "https://github.com/anolilab/javascript-style-guide.git",
        "directory": "packages/semantic-release-preset"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "type": "module",
    "main": ".releaserc-with-npm.json",
    "files": [
        "lib",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md",
        ".releaserc-with-npm.json",
        ".releaserc-without-npm.json"
    ],
    "scripts": {
        "postinstall": "node lib/postinstall.js"
    },
    "dependencies": {
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@commitlint/core": "^17.6.3",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^9.0.2",
        "@semantic-release/exec": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.0.0",
        "@semantic-release/release-notes-generator": "^11.0.1",
        "@semantic-release/npm": "^10.0.3",
        "commitizen": "^4.3.0",
        "conventional-changelog-conventionalcommits": "^5.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "semantic-release-conventional-commits": "^3.0.0"
    },
    "devDependencies": {
        "semantic-release": "^21.0.2"
    },
    "peerDependencies": {
        "semantic-release": "^21.0.0"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public"
    },
    "pnpm": {
