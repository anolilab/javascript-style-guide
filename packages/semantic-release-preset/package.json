{
    "name": "@anolilab/semantic-release-preset",
    "version": "3.1.1",
    "description": "Anolilab Coding Standard for semantic-release.",
    "keywords": [
        "anolilab",
        "semantic-release",
        "semantic-release-config",
        "changelog",
        "publish",
        "release",
        "npm",
        "github",
        "git"
    ],
    "homepage": "https://anolilab.com/nodejs/packages/semantic-release-preset",
    "repository": {
        "type": "git",
        "url": "https://github.com/anolilab/javascript-style-guide.git",
        "directory": "packages/semantic-release-preset"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/prisis"
        },
        {
            "type": "consulting",
            "url": "https://anolilab.com/support"
        }
    ],
    "license": "MIT",
    "author": {
        "name": "Daniel Bannert",
        "email": "d.bannert@anolilab.de"
    },
    "exports": {
        ".": {
            "require": "./config/without-npm.json",
            "import": "./config/without-npm.json"
        },
        "./npm": {
            "require": "./config/with-npm.json",
            "import": "./config/with-npm.json"
        },
        "./package.json": "./package.json"
    },
    "main": "config/without-npm.json",
    "source": "src/postinstall.ts",
    "files": [
        "dist",
        "README.md",
        "CHANGELOG.md",
        "LICENSE.md",
        "config/with-npm.json",
        "config/without-npm.json"
    ],
    "scripts": {
        "build": "cross-env NODE_ENV=development tsup",
        "build:prod": "cross-env NODE_ENV=production tsup",
        "clean": "rimraf node_modules dist",
        "postinstall": "node ./skip.js || node ./dist/postinstall.js"
    },
    "dependencies": {
        "@anolilab/package-json-utils": "1.3.0",
        "@commitlint/cli": "^17.6.5",
        "@commitlint/config-conventional": "^17.6.5",
        "@commitlint/core": "^17.6.5",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^10.0.1",
        "@semantic-release/exec": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.0.3",
        "@semantic-release/npm": "^10.0.4",
        "@semantic-release/release-notes-generator": "^11.0.3",
        "commitizen": "^4.3.0",
        "conventional-changelog-conventionalcommits": "^6.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "semantic-release-conventional-commits": "^3.0.0"
    },
    "devDependencies": {
        "semantic-release": "^21.0.4"
    },
    "peerDependencies": {
        "semantic-release": "^21.0.0"
    },
    "engines": {
        "node": ">=16"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    }
}
